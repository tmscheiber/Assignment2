@model IEnumerable<Group1_Assn4.Controllers.Q4Result>

@{
    ViewData["Title"] = "Question 4";
}
<p>4.	Which are the top 3 clients with most number of projects this year? </p>
<div class="row alert-info" style="padding:5px">
    @ViewData["LINQ"]
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ClientName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Revenue)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ClientName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Revenue)
                </td>

            </tr>
        }
    </tbody>
</table>
