@model IEnumerable<Group1_Assn4.Models.Resource>

@{
    ViewData["Title"] = "Question 8";
}
<p>8.	List down all the roles available?   </p>
<div class="row alert-info" style="padding:5px">
    @Html.Raw(@ViewData["LINQ"].ToString().Replace("^", "<br/>").Replace("_", "&emsp;"))
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Role)
                </td>

            </tr>
        }
    </tbody>
</table>
